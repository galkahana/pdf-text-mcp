# Example secret for API key authentication
# Copy this to secret.yaml and add your actual API key
# DO NOT commit secret.yaml to git!
apiVersion: v1
kind: Secret
metadata:
  name: pdf-text-mcp-secret
  namespace: pdf-text-mcp
  labels:
    app: pdf-text-mcp-server
type: Opaque
stringData:
  api-key: "your-secret-api-key-here"
